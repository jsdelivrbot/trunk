{% extends "@MarkdownWeb/layout.html.twig" %}

{% block menu %}
    {{ render_index("@MarkdownWeb/menu.html.twig", {"layout": "web.html"}, "priority:asc") }}
{% endblock %}

{% block content %}
    {% if page.jumbotron is defined %}
        <h1 class="hidden">{{ page.title }}</h1>
    {% else %}
        <div class="well">
            <h1>{{ page.title }}</h1>
        </div>
    {% endif %}
    {{ content|raw }}
{% endblock %}
