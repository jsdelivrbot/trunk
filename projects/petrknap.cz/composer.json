{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "symfony/cache": "^3.3",
        "symfony/http-foundation": "^3.3",
        "petrknap/php-servicemanager": "^1.0",
        "petrknap/php-migrationtool": "^2.0",
        "theiconic/php-ga-measurement-protocol": "^2.7",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "netpromotion/deployer": "^1.0",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "files": [
            "config/config.php"
        ],
        "psr-4": {
            "PetrKnapCz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PetrKnapCz\\Test\\": "tests"
        }
    }
}
