{# Core template #}
{% if count_of_pages != 1 %}
    <ul class="pagination">
        {% for p in 1..count_of_pages %}
            <li{% if p == page_number %} class="active"{% endif %}>
                <a href="{{ url }}?page={{ p }}">{{ p }}</a>
            </li>
        {% endfor %}
    </ul>
{% endif %}
