{
    "name": "petrknap/symfony",
    "type": "project",
    "license": "MIT",
    "homepage": "https://petrknap.github.io/docs/",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "dev@petrknap.cz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/Symfony/*"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/phpunit-bridge": "^3.2",
        "petrknap/symfony-markdownweb": "*"
    },
    "autoload": {
        "psr-4": {
            "PetrKnap\\Symfony\\MarkdownWeb\\": "packages/Symfony/MarkdownWeb/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PetrKnap\\Symfony\\MarkdownWeb\\Test\\": "packages/Symfony/MarkdownWeb/tests"
        }
    }
}
