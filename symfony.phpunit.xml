<phpunit bootstrap="./vendor/symfony/autoload.php">
    <testsuites>
        <testsuite>
            <directory suffix="Test.php">.</directory>
            <exclude>temp</exclude>
            <exclude>vendor</exclude>
        </testsuite>
    </testsuites>
    <listeners>
        <listener class="Symfony\Bridge\PhpUnit\SymfonyTestsListener"/>
    </listeners>
</phpunit>
